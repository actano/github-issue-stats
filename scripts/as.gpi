set xdata time
set timefmt "%Y-%m-%dT%H:%M:%SZ"
set datafile separator "\t"
#set terminal png size 768,768 enhanced truecolor font 'Roboto,9'
set terminal svg font 'Roboto,9'
set ylabel "Issues"
set xlabel "Date"

set pointsize 0.8
set format x "%Y"
set border 11
set xtics out
set tics front
set key below

plot \
  tsv using 1:($6+$7) title 'Stale' with filledcurves x1 lc rgb "#ff7700", \
  tsv using 1:6 title 'Active' with filledcurves x1 lc rgb "#009999" \
